<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.papa.dao.PmsProductVerifyDAO">
    <insert id="insertList">
        insert into `pms_product_verify_record`(product_id,create_time,verify_man,status,detail)
        values
            <foreach collection="list" separator="," index="index" item="it">
                (
                    #{it.productId,jdbcType=BIGINT},
                    #{it.createTime,jdbcType=TIMESTAMP},
                    #{it.verifyMan,jdbcType=VARCHAR},
                    #{it.status,jdbcType=INTEGER},
                    #{it.detail,jdbcType=VARCHAR}
                )
            </foreach>
    </insert>
</mapper>